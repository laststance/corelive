{
  "name": "corelive",
  "version": "0.1.0",
  "description": "A comprehensive design system and web application built with Next.js, featuring authentication, database integration, and desktop support",
  "author": "Ryota Murakami <dojce1048@gmail.com> (https://github.com/ryota-murakami)",
  "private": true,
  "type": "module",
  "main": "electron/main.cjs",
  "homepage": "./",
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c",
  "volta": {
    "node": "22.21.1"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{js,ts,jsx,tsx,cjs,mjs}": [
      "eslint --fix --max-warnings 0"
    ]
  },
  "scripts": {
    "postinstall": "pnpm prisma generate",
    "dev": "next dev -p 3011 --webpack",
    "build": "next build --webpack",
    "build:icons": "node scripts/generate-icons.js",
    "prebuild": "pnpm build:icons",
    "start": "next start -p 3011",
    "lint": "eslint . --concurrency=auto --max-warnings 0",
    "lint:fix": "eslint . --fix --concurrency=auto --max-warnings 0",
    "test": "vitest run",
    "test:electron": "vitest run --config vitest.config.electron.ts",
    "test:electron:watch": "vitest --watch --config vitest.config.electron.ts",
    "test:storybook": "vitest run --config vitest.config.storybook.ts",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "prettier": "prettier --ignore-unknown --write .",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf .next node_modules pnpm-lock.yaml tsconfig.tsbuildinfo",
    "prepare": "husky",
    "prisma:migrate": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma migrate dev",
    "prisma:status": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma migrate status",
    "prisma:deploy": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma migrate deploy",
    "prisma:validate": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma validate",
    "prisma:generate": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma generate",
    "prisma:studio": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma studio",
    "prisma:seed": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 tsx prisma/seed.ts",
    "db:reset": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma migrate reset --force",
    "db:truncate": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma migrate reset --skip-seed --force",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "e2e:web": "BASE_URL=http://localhost:3011 playwright test --project=web",
    "e2e:electron": "playwright test --project=electron",
    "electron": "electron .",
    "electron:dev": "node scripts/dev.js",
    "electron:dev:legacy": "concurrently \"pnpm dev\" \"wait-on http://localhost:3011 && node electron/dev-runner.cjs\"",
    "electron:build": "cross-env ELECTRON_BUILD=true next build --webpack && electron-builder",
    "electron:build:all": "cross-env ELECTRON_BUILD=true next build --webpack && electron-builder --mac --win --linux",
    "electron:build:win": "cross-env ELECTRON_BUILD=true next build --webpack && electron-builder --win",
    "electron:build:mac": "cross-env ELECTRON_BUILD=true next build --webpack && electron-builder --mac",
    "electron:build:linux": "cross-env ELECTRON_BUILD=true next build --webpack && electron-builder --linux",
    "electron:build:dir": "cross-env ELECTRON_BUILD=true next build --webpack && electron-builder --dir",
    "electron:build:optimized": "cross-env NODE_ENV=production ELECTRON_BUILD=true next build --webpack && electron-builder --config.compression=maximum",
    "electron:dist": "cross-env ELECTRON_BUILD=true next build --webpack && electron-builder --publish=never",
    "electron:publish": "cross-env ELECTRON_BUILD=true next build --webpack && electron-builder --publish=always",
    "electron:icons": "node scripts/generate-icons.js",
    "electron:build:prod": "node scripts/build.js",
    "electron:release": "node scripts/release.js",
    "electron:release:minor": "node scripts/release.js minor",
    "electron:release:major": "node scripts/release.js major",
    "preelectron:build": "pnpm typecheck && pnpm test:electron --run",
    "preelectron:publish": "pnpm typecheck && pnpm test:electron --run"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.35.5",
    "@hookform/resolvers": "^5.2.2",
    "@orpc/client": "^1.12.1",
