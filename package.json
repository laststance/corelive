{
  "name": "corelive",
  "version": "0.1.8",
  "description": "A comprehensive design system and web application built with Next.js, featuring authentication, database integration, and desktop support",
  "author": "Ryota Murakami <dojce1048@gmail.com> (https://github.com/ryota-murakami)",
  "private": true,
  "type": "module",
  "main": "dist-electron/main/index.cjs",
  "homepage": "./",
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48",
  "volta": {
    "node": "22.21.1"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{js,ts,jsx,tsx,cjs,mjs}": [
      "eslint --fix --max-warnings 0"
    ]
  },
  "scripts": {
    "postinstall": "pnpm prisma generate",
    "dev": "next dev -p 3011",
    "build": "next build --webpack",
    "build:icons": "node scripts/generate-icons.js",
    "prebuild": "pnpm build:icons",
    "start": "next start -p 3011",
    "lint": "eslint . --concurrency=auto --max-warnings 0",
    "lint:fix": "eslint . --fix --concurrency=auto --max-warnings 0",
    "test": "vitest run",
    "test:electron": "vitest run --config vitest.config.electron.ts",
    "test:electron:watch": "vitest --watch --config vitest.config.electron.ts",
    "test:storybook": "vitest run --config vitest.config.storybook.ts",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "prettier": "prettier --ignore-unknown --write .",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf .next node_modules pnpm-lock.yaml tsconfig.tsbuildinfo dist dist-electron dist-electron-dev",
    "prepare": "husky",
    "prisma:migrate": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma migrate dev",
    "prisma:status": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma migrate status",
    "prisma:deploy": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma migrate deploy",
    "prisma:validate": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma validate",
    "prisma:generate": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma generate",
    "prisma:studio": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma studio",
    "prisma:seed": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 tsx prisma/seed.ts",
    "db:reset": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma migrate reset --force",
    "db:truncate": "cross-env PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=1 prisma migrate reset --skip-seed --force",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "e2e:web": "APP_URL=http://localhost:3011 playwright test --project=web",
    "electron": "electron .",
    "electron:dev": "node scripts/dev.js",
    "electron:build:ts": "electron-vite build",
    "electron:build:mac": "pnpm electron:build:ts && cross-env APP_URL=https://corelive.app electron-builder --mac",
    "electron:build:dir": "pnpm electron:build:ts && cross-env APP_URL=https://corelive.app electron-builder --dir",
    "electron:publish": "pnpm electron:build:ts && cross-env APP_URL=https://corelive.app electron-builder --publish=always",
    "preelectron:publish": "pnpm typecheck && pnpm test:electron --run"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.36.8",
