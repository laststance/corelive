      "eslint --fix --max-warnings 0"
    ]
  },
  "main": "electron/main.cjs",
  "homepage": "./",
  "scripts": {
    "postinstall": "pnpm prisma generate",
    "dev": "next dev --turbo",
    "build": "next build",
    "build:icons": "node scripts/generate-icons.js",
    "prebuild": "pnpm build:icons",
    "start": "next start",
    "lint": "eslint . --concurrency=auto --max-warnings 0",
    "lint:fix": "eslint . --fix --concurrency=auto --max-warnings 0",
    "test": "vitest run",
    "test:electron": "vitest run --config vitest.config.electron.ts",
    "test:electron:watch": "vitest --watch --config vitest.config.electron.ts",
    "test:storybook": "vitest run --config vitest.config.storybook.ts",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "prettier": "prettier --ignore-unknown --write .",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf .next node_modules pnpm-lock.yaml tsconfig.tsbuildinfo",
    "prepare": "husky",
    "prisma:migrate": "prisma migrate dev",
    "prisma:status": "prisma migrate status",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:validate": "prisma validate",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "db:reset": "prisma migrate reset --skip-seed --force",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "e2e:web": "playwright test --project=web",
    "e2e:electron": "playwright test --project=electron",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "e2e:comprehensive": "node scripts/test-e2e-comprehensive.js",
    "electron": "electron .",
    "electron:dev": "node scripts/dev.js",
    "electron:dev:legacy": "concurrently \"pnpm dev\" \"wait-on http://localhost:3000 && node electron/dev-runner.cjs\"",
    "electron:build": "ELECTRON_BUILD=true pnpm build && electron-builder",
    "electron:build:all": "ELECTRON_BUILD=true pnpm build && electron-builder --mac --win --linux",
    "electron:build:win": "ELECTRON_BUILD=true pnpm build && electron-builder --win",
    "electron:build:mac": "ELECTRON_BUILD=true pnpm build && electron-builder --mac",
    "electron:build:linux": "ELECTRON_BUILD=true pnpm build && electron-builder --linux",
    "electron:build:dir": "ELECTRON_BUILD=true pnpm build && electron-builder --dir",
    "electron:build:optimized": "NODE_ENV=production ELECTRON_BUILD=true pnpm build && electron-builder --config.compression=maximum",
    "electron:dist": "ELECTRON_BUILD=true pnpm build && electron-builder --publish=never",
    "electron:publish": "ELECTRON_BUILD=true pnpm build && electron-builder --publish=always",
    "electron:icons": "node scripts/generate-icons.js",
    "electron:build:prod": "node scripts/build.js",
    "electron:release": "node scripts/release.js",
    "electron:release:minor": "node scripts/release.js minor",
    "electron:release:major": "node scripts/release.js major",
    "preelectron:build": "pnpm typecheck && pnpm test:electron --run",
    "preelectron:publish": "pnpm typecheck && pnpm test:electron --run"
  },
